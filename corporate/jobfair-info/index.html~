function ChangeImage (ImageName,FileName) {
// modified by Romesh Kumbhani so old Jscript based browsers don't choke
 var IsValid = (navigator.userAgent.indexOf("IE 2") == -1)&&(navigator.userAgent.indexOf("IE 3") == -1);
 if (IsValid) {
        document[ImageName].src = window[FileName].src;
 }
}


